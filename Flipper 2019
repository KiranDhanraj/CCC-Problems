#include <bits/stdc++.h>
using namespace std;

int
main () 
 { 
int grid[2][2] = { {1, 2}, {3, 4} };


string flip;

getline (cin, flip);

for (int i = 0; i < flip.size (); i++)
  {
	
if (flip[i] == 'V'){
	swap(grid[0][0], grid[0][1]);
    swap(grid[1][0], grid[1][1]);

 
}
	
if (flip[i] == 'H'){
	  
    swap (grid[0][0], grid[1][0]);
    swap (grid[0][1], grid[1][1]);
  
 
}
}
 
 


 
 
 
 
 
cout << grid[0][0] << " ";
  
cout << grid[0][1] << endl;
  
cout << grid[1][0] << " ";
  
cout << grid[1][1];
  
 
 
return 0;

}
